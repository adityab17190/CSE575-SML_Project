function [ ] = preProcessing()
%preProcessing Summary of this function goes here
%   Author = 'Tanmay Patil'

    inputFileNameTrain = 'data/train.csv';
    inputFileNameTest = 'data/test.csv';
    
    
    unique_a = [20,30,40,45,50,60,70,75,80,85,90,120,150,160,180,190];
    replace_a = {'A1','A2','A3','A4','A5','A6','A7','A8','A9','A10','A11','A12','A13','A14','A15','A16'};
    aColumnNumber = 2;
    
    step1NameTrain = 'data/train_step1.csv';
    step1NameTest = 'data/test_step1.csv';
    convertToCategoricalVars(inputFileNameTrain, step1NameTrain, unique_a, replace_a);
    convertToCategoricalVars(inputFileNameTest, step1NameTest);
    
    step2NameTrain = 'data/train_step2.csv';
    step2NameTest = 'data/test_step2.csv';
    dummyVar_impl(step1NameTrain, step2NameTrain);
    dummyVar_impl(step1NameTest, step2NameTest);

end

